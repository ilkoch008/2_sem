<div id="subscribe-quick-form" class="subscribe-quick" :class="{opened: formShown}">
    <div class="subscribe-quick-message-container" style="display: none" :style="{display: isReady ? 'block' : 'none'}">
        <div class="subscribe-quick-message" :class="{success: successMessage, error: errorMessage, shown: messageShown}" @click="hideMessage">
            {{successMessage || errorMessage}}
        </div>
    </div>
    <div style="overflow: hidden">
        <div class="subscribe-quick-button-container">
            <button class="subscribe-quick-button" @click="toggleForm">Быть в курсе событий</button>
        </div>
        <div class="subscribe-quick-form">
            <form @submit.prevent="submitHandler">
                <input type="hidden" name="site_id" value="s1" />
                <input type="hidden" name="asd_rubrics" value="" />
                <input type="hidden" name="asd_show_rubrics" value="N" />
                <input type="hidden" name="asd_not_confirm" value="N" />
                <div class="mf-email">
                    <input type="text" placeholder="Email адрес" v-model="email">
                </div>
                <input class="feedback_submit" type="submit" value="" :disabled="!email">
            </form>
        </div>
    </div>
</div>
<script>
    $(function() {
        new Vue({
            el: '#subscribe-quick-form',
            data: {
                isReady: false,
                saveProcess: false,
                successMessage: '',
                errorMessage: '',
                messageShown: '',
                formShown: '',
                rubricList: ["9"],
                timeoutHandle: null,
                email: ''
            },
            created: function() {
                this.isReady = true;
            },
            methods: {
                toggleForm: function() {
                    this.formShown = !this.formShown;
                },
                showMessage: function() {
                    if (!this.successMessage && !this.errorMessage) {
                        return;
                    }
                    this.messageShown = true;
                    clearTimeout(this.timeoutHandle);
                    this.timeoutHandle = setTimeout(function() {
                        this.hideMessage();
                    }.bind(this), 4000);
                },
                hideMessage: function() {
                    this.messageShown = false;
                    clearTimeout(this.timeoutHandle);
                    this.timeoutHandle = setTimeout(function() {
                        this.errorMessage = '';
                        this.successMessage = '';
                    }.bind(this), 200);
                },
                submitHandler: function() {

                    if (this.saveProcess) {
                        return;
                    }

                    var email = this.email.trim();

                    if (!email) {
                        return;
                    }

                    this.saveProcess = true;
                    this.hideMessage();

                    $.ajax({
                        url: '?method=subscribe',
                        type: 'post',
                        data: {
                            email: this.email,
                            rubricList: this.rubricList
                        },
                        dataType: 'json',
                        success: function(data) {
                            if (data.status === 'error') {
                                this.errorMessage = data.error_message ? data.error_message : 'Неизвестная ошибка';
                            }
                            if (data.status === 'ok' && data.data) {
                                this.successMessage = data.data;
                            }
                            this.showMessage();
                        },
                        error: function() {
                            this.errorMessage = 'Неизвестная ошибка';
                            this.showMessage();
                        },
                        complete: function() {
                            this.saveProcess = false;
                        },
                        context: this
                    });

                }
            }
        });
    });
</script>
