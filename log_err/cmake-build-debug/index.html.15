<script>
    var dt = {};
    dt.ajax_code_quote=1;
    dt.flag=true;
    dt.quote_temp_value="";
    function call_quote(){
        $("#quote_load img").addClass('animated rotateIn');
        current=$('#quote_id').val();
        dt.ajax_code_quote++;    
        if(!dt.flag) return;
        dt.flag=false;
        var data={
            controller:"get_iblock",
            fields:[
                'ID','NAME','CODE','DETAIL_TEXT'
            ],
            properties:[
            ],
            sort:{RAND:'DESC'},
            filter:{
                IBLOCK_ID:24,                
                current:{cond:'!=ID',val:current},                
            },
            number:1,
            other:{ajax_code:dt.ajax_code_quote}
        };
         $.ajax({
            url: '/bitrix/tools/ajax/',
            type: "POST",
            dataType: 'json',
            data: data,
            error: function () {},
            success: function (res) {
                var data=res.data;
                if(res.other.ajax_code!=dt.ajax_code_quote){
                    dt.flag=true; 
                  //  return false;                    
                }
               dt.quote_temp_value=data[0].DETAIL_TEXT;
               
                $("#quote-text div.qtxt").fadeOut(100,function(){
                    
                  $(this).empty().append(dt.quote_temp_value);
                  $(this).fadeIn(100,function(){
                      $("#quote_load img").removeClass('animated').removeClass('rotateIn');
                  });                  
                })
                $('#quote_id').val(data[0].ID);
                dt.flag=true;
            },
           //dataFilter: function(a,b){$('#news').empty().append(a)}
        });
    }
</script>

